#include <stdio.h>
#define SIZE 7

int hashTable[SIZE];

void insert(int key) {
    int index = key % SIZE;
    int i=0;
    while(hashTable[(index+i)%SIZE] != -1) i++;
    hashTable[(index+i)%SIZE] = key;
}

void display() {
    int i;
    printf("Hash Table: ");
    for(i=0;i<SIZE;i++)
        printf("%d ", hashTable[i]);
    printf("\n");
}

int main() {
    for(int i=0;i<SIZE;i++) hashTable[i]=-1;

    insert(10);
    insert(20);
    insert(15);
    insert(7);

    display();
    return 0;
}
