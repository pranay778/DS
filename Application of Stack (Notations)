#include <stdio.h>
#include <ctype.h>

#define SIZE 20
int stack[SIZE], top=-1;

void push(int x) { stack[++top]=x; }
int pop() { return stack[top--]; }

int main() {
    char exp[] = "23*54*+9-"; 
    int i, op1, op2;

    for(i=0; exp[i]!='\0'; i++) {
        if(isdigit(exp[i])) push(exp[i]-'0');
        else {
            op2 = pop(); op1 = pop();
            switch(exp[i]) {
                case '+': push(op1+op2); break;
                case '-': push(op1-op2); break;
                case '*': push(op1*op2); break;
                case '/': push(op1/op2); break;
            }
        }
    }
    printf("Result of Postfix Expression = %d\n", pop());
    return 0;
}
