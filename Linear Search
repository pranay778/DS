#include <stdio.h>
int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int key = 30, i, flag=0;

    for(i=0;i<5;i++) {
        if(arr[i] == key) {
            printf("Element %d found at position %d\n", key, i+1);
            flag=1;
            break;
        }
    }
    if(flag==0) printf("Element not found\n");
    return 0;
}
