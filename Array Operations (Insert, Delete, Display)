#include <stdio.h>
int main() {
    int arr[10] = {1,2,3,4,5}, n=5, i;

    printf("Initial Array: ");
    for(i=0;i<n;i++) printf("%d ", arr[i]);

    // Insert 99 at position 3
    for(i=n;i>=3;i--) arr[i]=arr[i-1];
    arr[2] = 99; n++;

    printf("\nAfter Insertion: ");
    for(i=0;i<n;i++) printf("%d ", arr[i]);

    // Delete element at position 4
    for(i=3;i<n-1;i++) arr[i]=arr[i+1];
    n--;

    printf("\nAfter Deletion: ");
    for(i=0;i<n;i++) printf("%d ", arr[i]);

    return 0;
}
